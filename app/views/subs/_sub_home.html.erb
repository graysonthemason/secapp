<div class="stock-header">
	<div class="hd">
		<div class="title">
			<h2><%= @sub.displayName %> <%= "[#{@sub.stock.ticker}]" if @hasStock %></h2>
			<span class="exchange"><%= "- #{@stock.exchange}" if @hasStock %></span>
		</div>
	</div>
	<div class="stock-quote-summary">
		<span class="bid"><%= @stockData[:bid] %></span>
		<div class=<%= "stock-state-#{@stockState}" %>>
			<i class="<%= "fa fa-caret-#{@stockState}"%>" aria-hidden="true"></i>
			<span class="price-change"><%= '%.2f' % @stockData[:change] %></span>
			<span class="price-change-percent">(<%= '%.2f' % @stockData[:change_in_percent] %>%)</span>
		</div>
	</div>

	<!-- UPDATE THIS -->
	<div class="After Hours">After Hours : <span id="yfs_l86_jbht">86.27</span></span> <span class="down_r"><span id="yfs_c85_jbht"><img width="10" height="14" style="margin-right:-2px;" border="0" src="http://l.yimg.com/os/mit/media/m/base/images/transparent-1093278.png" class="neg_arrow" alt="Down"> 0.11</span> <span id="yfs_c86_jbht">(0.12%)</span></span><span class="time_rtq"> <span id="yfs_t54_jbht">4:13PM EDT</span></span></div>
</div>

<ul>
<% @stockData.each do |row| %>
	<li><%= "#{row[0]} : #{row[1]}" %></li>
<% end %>
</ul>
