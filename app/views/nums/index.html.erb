<p id="notice"><%= notice %></p>

<h1>Listing Nums</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nums.each do |num| %>
      <tr>
        <td><%= link_to 'Show', num %></td>
        <td><%= link_to 'Edit', edit_num_path(num) %></td>
        <td><%= link_to 'Destroy', num, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Num', new_num_path %>
